    $(".input-group").on("click", ".button-plus", function (e) {
        !(function (e) {
            e.preventDefault();
            var t = $(e.target).data("field"),
                o = $(e.target).closest("div"),
                a = parseInt(o.find("input[name=" + t + "]").val(), 10);
            isNaN(a) ? o.find("input[name=" + t + "]").val(0) : o.find("input[name=" + t + "]").val(a + 1);
        })(e);
    }),
    $(".input-group").on("click", ".button-minus", function (e) {
        !(function (e) {
            e.preventDefault();
            var t = $(e.target).data("field"),
                o = $(e.target).closest("div"),
                a = parseInt(o.find("input[name=" + t + "]").val(), 10);
            !isNaN(a) && a > 0 ? o.find("input[name=" + t + "]").val(a - 1) : o.find("input[name=" + t + "]").val(0);
        })(e);
    });

$(document).ready(function () {

    (function () {
        const buttons = '.js-tab__button';
        const contents = '.js-tab__content';
        const activeClass = 'active';

        $(buttons).click(function (e) {
            e.preventDefault();

            const $container = $(this).closest('ul');
            const $contentsContainer = $('#' + $container.data('contents-id'));
            const tabId = $(this).data('tab');
            console.log(tabId);

            $container.find(buttons).removeClass(activeClass)
            $(this).addClass(activeClass);

            $contentsContainer.find(contents).hide().addClass('none');
            $contentsContainer.find('#' + tabId).stop().fadeIn();
        });
    })();

    (function () {
        $('.accordion-head').click(function () {
            $(this).toggleClass('active');
            $(this).next().slideToggle();
        });
    })();

    document.querySelectorAll('.tom-select').forEach((el)=>{
    	let settings = {};
     	new TomSelect(el,settings);
    });

    $(document).on('click', '.js-top-change', function(ev) {
        ev.preventDefault();
        $('.js-dash-top-place').html($(this).data('place'));
        $('.js-dash-top-users').html($(this).data('users'));
        $('.js-dash-top-rating').html($(this).data('rating'));
    });

    // Tutor panel filter
    $(document).on('change', '.js-chat-tutor-select', function(e) {
        var pick = this.options[this.selectedIndex].value;
        console.log(pick);
        location.href = pick;
    });

    // Видео плеер, для уроков
    if($('.js-player-init')){
        // const player = new Plyr('video');
        const player = new Plyr('video', {
            invertTime: false,
            i18n: {
                rewind: 'Rewind 5s',
                fastForward: 'Forward 5s',
            },
            controls: [
                'play-large', // The large play button in the center
                'restart', // Restart playback
                'rewind', // Rewind by the seek time (default 10 seconds)
                'play', // Play/pause playback
                'fast-forward', // Fast forward by the seek time (default 10 seconds)
                'progress', // The progress bar and scrubber for playback and buffering
                'current-time', // The current time of playback
                'mute', // Toggle mute
                'volume', // Volume control
                'captions', // Toggle captions
                'settings', // Settings menu
                'fullscreen', // Toggle fullscreen
            ],
            fullscreen: {iosNative:true}
        });

        var plyr = $('.js-player-init>.plyr');
        var overlay = $(".plyr-overlay")
        plyr.append(overlay);

        var PlayerTimer = 0;
        let PlayerInterval;

        function PlayerStartWatchTime() {
            if (!PlayerInterval) {
                PlayerInterval = setInterval(PlayerCountWatchTime, 1000);
            }
        }

        function PlayerStopWatchTime() {
            clearInterval(PlayerInterval);
            PlayerInterval = null;
        }

        function PlayerCountWatchTime() {
            PlayerTimer +=1;
            // Как только счетчик доходит до 10 отправляем запрос и сбрасываем до нуля
            if (PlayerTimer == '10') {
                $.ajax({
                    type: 'get',
                    url: window.location.href+"?watch=1",
                    success: function() {
                        // Обнуляем счетчик
                        PlayerTimer = 0;
                    }
                });
            }
        }

        // Запускаем счетчик при воспроизведении видео
        player.on('playing', (event) => {
            PlayerStartWatchTime();
        });

        // Останавливаем счетчик при паузе
        player.on('pause', () => {
            PlayerStopWatchTime();
        });

        // При клике на тайм-код на видео
        $(document).on('click', '.js-player-time-code', function(ev) {
            ev.preventDefault();
            player.currentTime = $(this).data('time');
        });
    }

    // Видео плеер, для стримов - Youtube
    if($('.js-plyr-youtube-init')){
        let player = new Plyr('.js-plyr-youtube', {
            invertTime: false,
            i18n: {
                rewind: 'Rewind 5s',
                fastForward: 'Forward 5s',
            },
            controls: [
                'play-large', // The large play button in the center
                'restart', // Restart playback
                'rewind', // Rewind by the seek time (default 10 seconds)
                'play', // Play/pause playback
                'fast-forward', // Fast forward by the seek time (default 10 seconds)
                'progress', // The progress bar and scrubber for playback and buffering
                'current-time', // The current time of playback
                'mute', // Toggle mute
                'volume', // Volume control
                'captions', // Toggle captions
                'settings', // Settings menu
                'fullscreen', // Toggle fullscreen
            ]
        });
    }

    // Открыть таб календаря в дашборде
    $(document).on('click', '.js-calendar-open-tab', function(ev) {
        $('.js-tab__button_calendar').trigger('click');
    });

    // Загрузка аватарки
    $(document).on('click', '.js-avatar-upload', function(ev) {
        $('#user_avatar').trigger('click');
    });

    // Удаление аватарки
    $(document).on('click', '.js-avatar-delete', function(ev) {
        $('#delete_image').attr('name', 'delete_image').attr('checked', true).val('1');
        $('.user-form').submit();
    });

    // Открытие фото в методичках по клику
    $(document).on('click', '.lab-body img', function(ev) {
        ev.preventDefault();
        var url = $(this).attr('src');
        const fancybox = new Fancybox([{
            src: url,
            type: "image",
        }]);
        return false;
    });

});

var ChatEditor;
var ChatRefreshTimer;
var ChatMessagesOffset = 0;
var ChatCursorPosition = 0;
var ChatAutoScroll = true;
let ChatFiles = [];
var ChatForm = $('.js-chat-form');
let ChatType = $('.js-chat-component').data('type');
let ChatID = ChatForm.find('input[name=chat_id]').val();
let LessonID = ChatForm.find('input[name=lesson_id]').val();
let ReplyContainer = $('.reply-container');
let EditContainer = $('.edit-container');
let TemplatesContainer = $('.templates-container');

// Текстовый редактор для чата
function ChatEditorInit(){

    // Инициализируем чат с конфигом
    ChatEditor = new MediumEditor('.js-chat-editor', {
        toolbar: {
            buttons: [
                {
                    name: 'bold',
                    action: 'bold',
                    aria: 'Жирний',
                    contentDefault: '<b>B</b>',
                },
                {
                    name: 'italic',
                    action: 'italic',
                    aria: 'Курсив',
                    contentDefault: '<i>I</i>',
                },
                {
                    name: 'underline',
                    action: 'underline',
                    aria: 'Підкреслений',
                    contentDefault: '<u>U</u>',
                },
                {
                    name: 'strikethrough',
                    action: 'strikethrough',
                    aria: 'Закреслений',
                    contentDefault: '<s>S</s>',
                },
            ],
        },
        hideOnClick: false,
        placeholder: {
            text: 'Написати повідомлення..'
        }
    });

    // При любых изменениях забираем данные из редактора и вставляем в скрытую textarea
    ChatEditor.subscribe('editableInput', function (event, editable) {

        data = ChatEditor.getContent();

        // Chat clean fix
        if (data == '<p><br></p>')
            ChatEditor.setContent('', 0);

        // Chat clean fix
        if (data == '<br>')
            ChatEditor.setContent('', 0);

        // Check if data contains <img> tag
        if (data.indexOf('img') !== -1) {
            alert('Зображення потрібно вставляти через кнопку!');
            ChatEditor.setContent('', 0);
        }

        // Copy val to textarea
        UpdateChatEditorContent();

    });

    // Отправка сообщения в чат по Shift + Enter
    ChatEditor.subscribe("editableKeydownEnter", function (event, element) {
        // Проверяем, нажат ли shift и enter
        if (event.shiftKey && event.keyCode === 13) {
            // Если да, создаем новую строку
            document.execCommand('insertHTML', false, '<p></p>');
        } else if (event.keyCode === 13) {
            // Если только enter, вызываем функцию SetMessage
            event.preventDefault();
            // Оптравим форму
            $('.js-chat-btn-submit').trigger('click');
            // Очистим редактор
            ChatEditorClean(ChatEditor);
            event.preventDefault();
            event.stopPropagation();
        }
    });

    // Вернем редактор для будущей работы с ним
    return(ChatEditor);
}

// Очистка чата
function ChatEditorClean(editor){
    editor.setContent('', 0);
}

// Получение старых сообщений чата
function LoadScrollChatMessages(){
    $.ajax({
        url: "ajax/chat.php",
        type: "post",
        data : {
            "action": "load",
            "type": ChatType,
            "chat_id": ChatID,
            "lesson_id": LessonID,
            "offset": ChatMessagesOffset
        },
        success: function(response) {
            if (response.messages) {
                const messages = Array.from(response.messages);
                messages.forEach(function(item) {
                    // Проверяем, есть ли уже сообщение с таким же идентификатором
                    if ($('.chat-message-item[data-message-id="' + item.id + '"]').length == 0) {
                        // Если сообщения с таким идентификатором нет, то вставляем его в блок чата
                        $(".js-chat-content").append(response.content);
                    }
                });
                ChatMessagesOffset = 0;
            }
        },
        complete: function() {
            // $('.js-chat-preloader').addClass('d-none');
        }
    });
}

// Получение новых сообщений чата
function LoadNewChatMessages(){
    let LastMessageId = $('.chat-message-item:first').data('message-id');
    if (!LastMessageId) {
        LastMessageId = 0;
    }
    $.ajax({
        url: "ajax/chat.php",
        type: "post",
        data : {
            "action": "update",
            "type": ChatType,
            "chat_id": ChatID,
            "lesson_id": LessonID,
            "message_id": LastMessageId
        },
        success: function(response) {
            // New messages
            if (response && response.messages && response.messages.length > 0) {
                $(".js-chat-content").prepend(response.content);
                //
                if ($('.chat-message-item').length > 12) {
                    $(".js-chat-new-messages").removeClass('d-none');
                }
            }
            // Update chat rating
            if (ChatType == 'lesson')
            {
                if(rate = response.chat.rating)
                {
                    // Закроем форму чтобы нельзя было больше отправить сообщение
                    $('.js-chat-form').html('');

                    // Пофиксим высоту чата так как формы уже нет
                    $('.chat').addClass('closed');

                    // Выставим оценку числом и текстом
                    const rating = {1: 'Очень плохо', 2: 'Плохо', 3: 'Средне', 4: 'Хорошо', 5: 'Отлично'};
                    $('.js-lesson-rating-value').html(rate+'/5');
                    $('.js-lesson-rating-text').html(rating[rate]);

                    // Выставим звездный рейтинг
                    $('.js-star').removeClass('text-warning').removeClass('text-light');
                    for (var r = 1; r < 6; r++) {
                        $('.js-star-'+r).addClass(rate < r ? 'text-light' : 'text-warning');
                    }

                    // покажем блок с оценкой
                    $('.js-lesson-rating-block').removeClass('d-none');
                    // покажем блок с текстом при оценке
                    $('.js-lesson-info-block').removeClass('d-none');
                }
            }
            // Update chat panel
            if($("#chats").length > 0){
                //обновить миничат
                let item = $('.js-chat-item[data-id="' + response.chat.id + '"]');
                item.find('.js-chat-last-message').text(response.chat.last_message.substring(0, 24)+'...');
                item.find('.js-chat-last-added').html(response.chat.last_added);
            }
        },
        complete: function() {
            $('.js-chat-preloader').addClass('d-none');
            ChatRefreshTimer = setTimeout(LoadNewChatMessages, 36000); // 10 seconds
            // Cкроллим вниз
            if(ChatAutoScroll == true)
                ChatScrollBottom();
        }
    });
}

// Update sender info
function UpdateSenderInfo(){
    $.ajax({
        url: "ajax/chat.php",
        type: "post",
        data : {
            "action": "update_sender",
            "type": ChatType,
            "chat_id": ChatID,
            "lesson_id": LessonID
        },
        success: function(response) {
            // Обновим статус юзера
            let status = response.content;
            if (status == 'offline') {
                $('.js-chat-sender-avatar').removeClass('avatar-online').addClass('avatar-offline');
                $('.js-chat-sender-status').addClass('text-muted').removeClass('text-success').html('Offline');
            }
            if (status == 'online') {
                $('.js-chat-sender-avatar').removeClass('avatar-offline').addClass('avatar-online');
                $('.js-chat-sender-status').removeClass('text-muted').addClass('text-success').html('Online');
            }
        }
    });
}

// Reset edit
function EditReset(){
    EditContainer.hide();
    EditContainer.find('.edit-message').html('');
    $('input[name=message_id]').val('');
}

// Reset reply
function ReplyReset(){
    ReplyContainer.hide();
    ReplyContainer.find('.reply-name').html('');
    ReplyContainer.find('.reply-message').html('');
    $('input[name=reply_id]').val('');
}

// Scroll chat to bottom
function ChatScrollBottom(){
    $(".js-chat-scroll-container").scrollTop($(".js-chat-scroll-container")[0].scrollHeight);
}

function CheckChatScroll(){
    $('.js-chat-scroll-container').on('scroll', function() {
        var container = $('.js-chat-scroll-container');
        var scrollHeight = container.prop('scrollHeight');
        var scrollTop = container.scrollTop();
        var containerHeight = container.innerHeight();

        if (scrollTop + containerHeight >= scrollHeight)
        {
            ChatAutoScroll = true;
        } else {
            ChatAutoScroll = false;
        }
    });
}

// Update textarea
function UpdateChatEditorContent(){
    data = ChatEditor.getContent();
    $('.js-chat-textarea').val(data);
}

// Save cursor position
function SaveChatCursorPosition(){
    var selection = window.getSelection();
    range = selection.getRangeAt(0);
    ChatCursorPosition = range.startOffset;
}

// Показ и скрытие модалки превью файлов
function CheckChatFilesLength(files) {
    if (files && files.length > 0) {
        $('.js-files-attachment-modal').removeClass('d-none');
        $('.js-chat-attachment-count').removeClass('d-none');
        $('.js-chat-attachment-count').html(files.length);
    } else {
        $('.js-files-attachment-modal').addClass('d-none');
        $('.js-chat-attachment-count').addClass('d-none');
        $('.js-chat-attachment-count').html('');
    }
}

// Очистка всех загружных файлов в чат
function RemoveAllChatFiles() {
    // Очистим ошибку файлов
    $('.js-files-attachment-modal').find('.alert').html('').addClass('d-none');
    // Очистим модалку с превью
    $('#js-attachment-list').html('');
    // Очистим инпуты
    $('#js-chat-attachment').val('');
    $('.js-chat-files').val('');
    // Очистим переменную списка файлов
    ChatFiles = [];
}

// Функция загрузки файлов в чат
function UploadChatFiles(files) {
    if (ChatType == 'lesson' || ChatType == 'tutor') {

        let ChatNewFiles = [];
        for(let index = 0; index < files.length; index++) {
            // Максимальный размер файла 2 МБ
            let maxFileSize = 2 * 1024 * 1024;
            let file = files[index];

            if(file.size > maxFileSize){
                // Покажем модалку, возможно это единичный файл для загрузки
                $('.js-files-attachment-modal').removeClass('d-none');
                // Отобразим ошибку
                $('.js-files-attachment-modal').find('.alert').append('Максимальний розмір файлу складає 2 МБ. Файл <b>'+file.name+'</b> пропущений.').removeClass('d-none');
            } else {
                if (file.type == 'image/jpeg' || file.type == 'image/png' || file.type == 'image/gif') {
                    // Тут можно сделать проверку на размер файла
                    // и при необходимости не грузить большие файлы
                    ChatNewFiles.push(file);
                    ChatFiles.push(file);
                    CheckChatFilesLength(ChatFiles);
                }else{
                    // Покажем модалку, возможно это единичный файл для загрузки
                    $('.js-files-attachment-modal').removeClass('d-none');
                    // Отобразим ошибку
                    $('.js-files-attachment-modal').find('.alert').append('До завантаження допускаються лише файли типу JPG, PNG, GIF. Файл <b>'+file.name+'</b> пропущений.').removeClass('d-none');
                }

            }
        }

        ChatNewFiles.forEach(file => {
            // Обработка типа файла
            let FileTypeOrg = file.name.split('.').pop();
            const FileTypes = {
                'jpg': 'image',
                'png': 'image',
                // 'webp': 'image',
                'gif': 'image',
                // 'mp3': 'filetype-mp3',
                // 'mkv': 'filetype-mov',
                // 'mp4': 'filetype-mp4',
                // 'mov': 'filetype-mov',
                // 'pdf': 'filetype-pdf',
                // 'xls': 'filetype-xls',
                // 'xlsx': 'filetype-xlsx',
                // 'doc': 'filetype-doc',
                // 'docx': 'filetype-docx',
                // 'rar': 'file-earmark-zip',
                // 'zip': 'file-earmark-zip',
            };
            // Определим иконку файла
            let FileIcon = FileTypes[FileTypeOrg];
            // Для изображений формируем вывод через blob
            // в остальных случаях выводим соответствующую иконку
            if (FileIcon == 'image' ) {
                let img = URL.createObjectURL(file);
                var IconBlock = "<img src="+img+" class='img-fluid'>";
            } else {
                var IconBlock = "<i class='bi bi-"+FileIcon+" fs-3'></i>";
            }
            // Формируем блок каждого элемента
            let fileElement = $(`
            <div class="item" data-index="${file}">
                <div class="d-flex align-items-center">
                    <div class="icon-shape bg-light text-primary icon-sm rounded-circle me-2">
                        ${IconBlock}
                    </div>
                    <div class="text-truncate">${file.name}</div>
                </div>
                <div class="text-truncate">
                    <button type="button" class="btn btn-icon btn-outline-danger js-chat-image-delete" data-bs-toggle="tooltip" data-placement="top" title="Видалити">
                        <i class="bi bi-trash"></i>
                    </button>
                </div>
            </div>
            `);
            // Запишем атрибут каждому файлу для возможности удаления по ключу
            fileElement.data('fileData', file);
            // Вставляем превью файла в модальное окно
            $('#js-attachment-list').append(fileElement);
            // Определим кнопку для удаления файла из модального окна
            let ElementRemove = fileElement.find('.js-chat-image-delete');
            ElementRemove.click(function(event) {
                let fileElement = $(event.target);
                let indexToRemove = ChatFiles.indexOf(fileElement.data('fileData'));
                fileElement.closest('.item').remove();
                ChatFiles.splice(indexToRemove, 1);
                CheckChatFilesLength(ChatFiles);
            });
        });
        // CheckChatFilesLength(ChatFiles);
    }
}

// Загрузка файлов в чат по CTRL + V
window.addEventListener("paste", function(e) {
    if (ChatType == 'lesson' || ChatType == 'tutor') {
        if (e.clipboardData.files.length > 0) {
            // Определим файлы чтобы передать в основную функцию
            let files = e.clipboardData.files;
            // Инициализируем функцию загрузки файлов
            UploadChatFiles(files);
        }
    }
});

function CheckMessageFill(ChatEditor){
    var status = false;
    data = ChatEditor.getContent();
    // Удаляем все пробельные символы и теги из сообщения
    var messageWithoutTags = data.replace(/<[^>]+>/g, '');
    var messageWithoutSpaces = messageWithoutTags.replace(/\s|&nbsp;/g, '');
    // console.log('Message:', messageWithoutSpaces);

    // Files
    var files = ChatFiles.length;
    // console.log('Files:', files);

    // Если сообщение и файлы пустые
    if (messageWithoutSpaces === '' && files === 0) {
        status = false;
    }
    // Если сообщение есть, а файлов нет
    else if (messageWithoutSpaces !== '' && files === 0) {
        status = true;
    }
    // Если сообщения нет, а файлы есть
    else if (messageWithoutSpaces === '' && files > 0) {
        status = true;
    }
    else
    {
        status = true;
    }
    // console.log('Status:', status);
    return status;
}

$(document).ready(function () {

    // Chat editor
    let ChatEditor = ChatEditorInit();
    var ChatSendAction = 'add';

    // Если чат активен получим сообщения и подключим редактор
    if($('.js-chat-component-init').length)
    {
        // Chat type
        console.warn('ChatType:', ChatType);

        if (ChatType == 'lesson' || ChatType == 'tutor') {

            // Обновляем статус собеседника
            UpdateSenderInfo();
            setInterval(UpdateSenderInfo, 60000);
        }

        // Получим новые сообщения
        LoadNewChatMessages();
        CheckChatScroll();

        // Emoji
        const pickerOptions = {
            locale: 'ru',
            theme: 'light',
            previewPosition: 'none',
            onEmojiSelect: function(emoji) {
                // добавляем эмодзи в редактор
                var emoji = emoji.native;
                var editor = $('.js-chat-editor');
                var existing_text = editor.text();
                var position = ChatCursorPosition;
                var data = existing_text.slice(0, position) + emoji + existing_text.slice(position);
                editor.text(data);

                // Выставляем курсор после вставленого emoji
                var sel = window.getSelection();
                var range = document.createRange();
                range.setStart(editor[0].childNodes[0], position+1);
                range.collapse(true);
                sel.removeAllRanges();
                sel.addRange(range);
                editor.focus();

                // Обновим редактор
                UpdateChatEditorContent();
            }
        }
        const picker = new EmojiMart.Picker(pickerOptions);
        $('.emojis-contain').append(picker);
    }

    // При скролле подгружаем старые сообщения
    $('.js-chat-scroll-container').scroll(function(event) {
        if ($(this).scrollTop() <= 140 && ChatMessagesOffset == 0) {
            // $('.js-chat-preloader').removeClass('d-none');
            ChatMessagesOffset = $('.js-chat-content').children().length;
            LoadScrollChatMessages();
        }
        // Если проскроллен до низа уберем кнопку
        if($(this).scrollTop() + $(this).innerHeight() >= $(this)[0].scrollHeight) {
            $(".js-chat-new-messages").addClass('d-none');
        }
        return;
    });

    // При клике на кнопку файлов имитируем клик по инпуту
    $(document).on('click', '.js-chat-btn-attach', function(ev) {
        $('.js-chat-attachment').trigger('click');
    });

    // Загрузка файлов в чат по кнопке
    $(document).on('change', '#js-chat-attachment', function() {
        // Определим файлы чтобы передать в основную функцию
        let files = $(this)[0].files;
        // Инициализируем функцию загрузки файлов
        UploadChatFiles(files);
    });

    // Удаление всех загружаемых изображений чата
    $(document).on('click', '.js-chat-all-img-delete', function() {
        // Очистка всех загружных файлов в чат
        RemoveAllChatFiles();
        // Пересчитаем кол-во файлов
        CheckChatFilesLength();
    });

    // Скролл вниз по клику
    $(document).on('click', '.js-chat-scroll-bottom', function(ev) {
        ev.preventDefault();
        var chatContainer = $(".js-chat-scroll-container");
        chatContainer.animate({ scrollTop: chatContainer.prop("scrollHeight") }, 1000);
        $('.js-chat-new-messages').addClass('d-none');
    });

    // Отправка сообщения в чат
    $(document).on('submit', '.js-chat-form', function(ev) {
        ev.preventDefault();
        // Покажем прелоудер
        $('.js-chat-preloader').removeClass('d-none');
        // Соберем форму
        let formData = new FormData(this);
        formData.append("action", ChatSendAction);
        formData.append("type", ChatType);
        // Соберем все файлы
        ChatFiles.forEach(file => {
            formData.append('attach[]', file);
        });
        // Check on empty
        if (!CheckMessageFill(ChatEditor)) {
            alert('Помилка! Не можна надсилати порожні повідомлення!');
            return false;
        }
        $.ajax({
            url: "ajax/chat.php",
            type: "post",
            async: false,
            data: formData,
            processData: false,
            contentType: false,
            success: function(response) {
                // Вставим добавление сообщение в блок чата
                if (response.content) {
                    $(".js-chat-content").prepend(response.content);
                }
                if (response.status == 'edited') {
                    $('.chat-message-item[data-message-id="' + response.content.id + '"]').find('.message').html(response.content.text);
                }
                // Сбросим форму
                $('.js-chat-form')[0].reset();
                // Reset action
                ChatSendAction = 'add';
                // Очистим редактор чата
                ChatEditorClean(ChatEditor);
                // Очистка всех загружных файлов в чат
                RemoveAllChatFiles();
                // Обновим кол-во файлов
                CheckChatFilesLength();
                // Очистим цитирование
                ReplyReset();
                // Очистим редактирование
                EditReset();
                // Обновим чат со скроллом вниз
                // LoadNewChatMessages(true);
                ChatScrollBottom();
                // Уберем прелоудер
                $('.js-chat-preloader').addClass('d-none');
            }
        });
    });

    // При клике по редактору сохраняем позицию курсора
    $(document).on('click', '.js-chat-editor', function() {
        SaveChatCursorPosition();
    });

    // Показываем модалку с emoji
    $(document).on('click', '.js-chat-emoji-modal', function(ev) {
        ev.preventDefault();
        $('.emojis-contain').toggleClass('open');
    });

    // Скрываем модалку с emoji по клику вне блока
    $(document).on('mouseup', function(e){
        let modal = $('.emojis-contain.open');
        if(!modal.is(e.target) && modal.has(e.target).length === 0) {
            modal.removeClass('open');
        }
    });

    // Редактирование
    $(document).on('click', '.js-chat-message-edit', function(ev) {
        ev.preventDefault();
        ChatSendAction = 'edit';
        let message = $(this).closest('.chat-message-item');
        let id = message.data('message-id');
        let content = message.find('.message').html();
        // Очистим редактор
        ChatEditorClean(ChatEditor);
        // Заменим контент редактора
        ChatEditor.setContent(content);
        // Запишем ID
        $('input[name=message_id]').val(id);
        // Покажем блок
        EditContainer.find('.edit-message').html(content);
        EditContainer.show();
    });

    // Удалить редактирование
    $(document).on('click', '.js-chat-reset-edit', function(ev) {
        ev.preventDefault();
        EditReset();
        ChatSendAction = 'add';
    });

    // Цитирование
    $(document).on('click', '.js-chat-message-reply', function(ev) {
        ev.preventDefault();
        let message = $(this).closest('.chat-message-item');
        let name = message.data('reply-name');
        let text = message.find('.message').html();
        ReplyContainer.find('.reply-name').html(name);
        ReplyContainer.find('.reply-message').html(text);
        $('input[name=reply_id]').val(message.data('message-id'));
        // Покажем блок с цитированием
        ReplyContainer.show();
    });

    // Удалить цитирование
    $(document).on('click', '.js-chat-reset-reply', function(ev) {
        ev.preventDefault();
        ReplyReset();
    });

    // Удалить сообщение
    $(document).on('click', '.js-chat-message-delete', function(ev) {
        ev.preventDefault();
        let message = $(this).closest('.chat-message-item');
        $.ajax({
            url: "ajax/chat.php",
            type: "post",
            data : {
                "action": "delete",
                "type": ChatType,
                "chat_id": ChatID,
                "lesson_id": LessonID,
                "message_id": message.data('message-id')
            },
            success: function(response) {
                if (response.content == true) {
                    message.remove();
                }
            }
        });
    });

    // Выставление оценки ДЗ
    $(document).on('submit', '.js-chat-setrate', function(ev) {
        ev.preventDefault();
        let modal = $(this).closest('.modal-body');
        let rate = $(modal).find('select[name=rate]').val();
        $.ajax({
            url: "ajax/chat.php",
            type: "post",
            data: {
                "action": "rating",
                "type": ChatType,
                "chat_id": ChatID,
                "rating": rate
            },
            success: function(response) {
                $(modal).find('.alert-success').removeClass('d-none');
                $(modal).find('.js-chat-setrate').addClass('d-none');
                location.reload();
            }
        });
        return false;
    });

    // При клике на таб в уроке сроклим чат
    $(document).on('shown.bs.tab', 'a[href="#chats"]', function(e) {
        ChatScrollBottom();
    });

    // Показать шаблонны
    $(document).on('click', '.js-chat-templates-open', function(ev) {
        ev.preventDefault();
        TemplatesContainer.toggle();
    });

    // Закрыть шаблоны
    $(document).on('click', '.js-chat-close-templates', function(ev) {
        TemplatesContainer.hide();
    });

    // Применить шаблон
    $(document).on('click', '.js-chat-template-item', function(ev) {
        ev.preventDefault();
        ChatEditor.setContent($(this).html());
        $('.js-chat-editor').focus();
    });

});

$(document).ready(function () {

    // При следующем вопросе релоудим страницу
    $(document).on('click', '.js-test-next-answer', function() {
        window.location.reload();
    });

    // Подключим таймер только когда ученик находится на страницах вопросов
    if ($('.js-test-timer-init').length) {
        // Получим текущее время в секундах
        var timer = $('.js-test-timer').data('start');
        // Запустим таймер
        timerInterval = setInterval(function(){
            timer += 1;
            // Выведем таймер в формате ч:м:с
            var hours = Math.floor(timer / 3600).toString().padStart(2, '0');
            var minutes = Math.floor(timer / 60 % 60).toString().padStart(2, '0');
            var seconds = (timer % 60).toString().padStart(2, '0');
            $('.js-test-timer').html(hours + ":" + minutes + ":" + seconds);
            // Покаже блок таймера
            $('.js-test-timer-block').removeClass('d-none');
        }, 1000);
    }

    // Счетчик обратного отсчета повтора сдачи
    if ($('.js-test-countdown').length) {
        // Получим текущее время в секундах
        var time = $('.js-test-countdown').data('time');
        // Разбиваем время на часы, минуты и секунды
        var timeParts = time.split(":");
        var hours = parseInt(timeParts[0]);
        var minutes = parseInt(timeParts[1]);
        var seconds = parseInt(timeParts[2]);

        function updateCountdown() {
            // Отнимаем 1 секунду
            seconds -= 1;
            // Корректируем значения минут и часов
            if (seconds < 0) {
                seconds = 59;
                minutes -= 1;
            }
            if (minutes < 0) {
                minutes = 59;
                hours -= 1;
            }
            var formattedTime = ("0" + hours).slice(-2) + ":" + ("0" + minutes).slice(-2) + ":" + ("0" + seconds).slice(-2);
            $('.js-test-countdown').html(formattedTime);

            if (formattedTime === '00:00:00') {
                window.location.reload();
            }
        }
        setInterval(updateCountdown, 1000);
    }

    // Стилизация ответов теста
    $(document).on('click', '.js-test-label', function(ev) {
        let input = $(this).find('input[type="radio"]');
        if (!input.prop('disabled')) {
            $('.js-test-label').removeClass('border-success');
            $(this).addClass('border-success');
        }
    });

    // Подтвердить свой ответ
    $(document).on('submit', '.js-test-form', function(ev) {
        ev.preventDefault();
        // Скроем кнопку сабмита
        $('.js-test-submit-answer').addClass('d-none');
        // Определим выбранный ответ
        let answer = $(this).find('input[name=answer]:checked').val();
        // Запретим последующий выбор
        $('.js-test-form').find('input[type="radio"]').attr('disabled', true);
        $.ajax({
            url: window.location.href,
            type: "post",
            data: {
                answer: answer
            },
            success: function(response) {
                // Покажем кнопку следующего вопроса
                $('.js-test-next-answer').removeClass('d-none');
                // Проверим правильно ли ответил ученик
                if (response == "true") {
                    // Выведем алерт о верном ответе
                    $(".js-test-success-block").removeClass("d-none");
                } else {
                    // Выведем алерт о неправильном ответе
                    $(".js-test-alert-block").removeClass("d-none");
                    // Отобразим нужное пояснение, пояснение выбранного ответа
                    $(".js-test-hint-" + answer).removeClass("d-none");
                }
            },
            error: function(xhr, status, error) {
                window.location.reload();
                // var errorMessage = xhr.responseText;
                // alert("Произошла ошибка: " + errorMessage);
            }
        });
        return false;
    });

});

