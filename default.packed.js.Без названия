        e.fn.downCount = function (t, o) {
            var a = e.extend({ date: null, offset: null }, t);
            a.date || e.error("Date is not defined."), Date.parse(a.date) || e.error("Incorrect date format, it should look like this, 12/24/2012 12:00:00.");
